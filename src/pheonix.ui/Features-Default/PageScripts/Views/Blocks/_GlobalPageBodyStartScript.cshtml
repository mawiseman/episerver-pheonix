@using Features.PageScripts.Models.Pages
@model EPiServer.Core.PageData
@{
    if (!(Model is IHasGlobalPageScripts) || EPiServer.Editor.PageEditing.PageIsInEditMode)
    {
        return;
    }

    var pageScripts = ((IHasGlobalPageScripts)Model).GlobalPageScripts;
}
@if (!string.IsNullOrEmpty(pageScripts.PageBodyStartScript))
{
    @Html.Raw(pageScripts.PageBodyStartScript)
}